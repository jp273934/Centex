@using System.Net.Http.Formatting
@using System.Web.Mvc.Html

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="Centex Material Handling. Material handling company located in Leander Texas" />
    <meta name="author" content="Jeremy Parris" />
    <meta name="keywords" content="material, handling, austin, tx, leander, cedar park, centex" />

    <!-- Material Design fonts -->
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Roboto:300,400,500,700">
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/icon?family=Material+Icons">
    <link href="https://fonts.googleapis.com/css?family=Alegreya+SC:700,900" rel="stylesheet">
    <link href="~/Content/styles.css" rel="stylesheet" />

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/node_modules/bootstrap-material-design/dist/css/bootstrap-material-design.min.css" rel="stylesheet" />
    <link href="~/node_modules/bootstrap-material-design/dist/css/ripples.min.css" rel="stylesheet" />
    <script src="~/Scripts/angular.min.js"></script>


    <script src="~/Scripts/jquery-3.1.1.min.js"></script>

    <script src="~/node_modules/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/node_modules/bootstrap-material-design/dist/js/material.min.js"></script>
    <script src="~/node_modules/bootstrap-material-design/dist/js/ripples.min.js"></script>

    
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    
    <script src="~/Scripts/Modules/app.js"></script>
    <script src="~/Scripts/Controllers/ContactCtrl.js"></script>
    
    <script>
        $(document).ready(function() {
            $.material.init();          
        });

    </script>
    <title>Centex Material Handling</title>
    
</head>

<body style="background-color: #fff;">

<div class="row navbar-fixed-top" style="background-color: #000;">
    <div class="col-xs-12 col-md-7 col-sm-7  col-lg-3 col-lg-offset-3" style="display: none;">
        <span style="" >Centex Material Handling</span>
    </div>
    <div class="col-sm-5 col-md-5 col-lg-2 col-xs-12" style="display: none;">
        <address>
            <span style="color: #fff; font-size: 1.5em; font-family: Copperplate, Copperplate Gothic Light, fantasy;">AUSTIN, TEXAS</span><br />
        </address>
       
        <span style="color: #fff; font-size: 1.5em; font-family: Copperplate, Copperplate Gothic Light, fantasy;">PH: 512-337-8181</span><br/>
        <span style="color: #fff; font-size: 1.5em; font-family: Copperplate, Copperplate Gothic Light, fantasy;">FAX: 512-646-2661</span>
    </div>
    <div class="col-lg-offset-1 col-lg-12">
        @*<img src="~/Images/Header/CentexHeaderOrange.png" class="header"/>*@
        @*<img src="~/Images/Header/CentexHeaderOrange.svg" class="header" />*@
        <object type="image/svg+xml" data="~/Images/Header/CentexHeaderOrange.svg" class="header"></object>
    </div>
</div>

<nav class="navbar navbar-default navbar-fixed-top" id="navigation">
    <div class="container-fluid">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @*<a class="navbar-brand" href="#">Centex Material Handling</a>*@
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="navbar-collapse collapse navbar-responsive-collapse" id="bs-example-navbar-collapse-1">

            <ul class="nav navbar-nav navbar-center" style="margin-top: 1vh;">
                <li>@Html.ActionLink("Home", "Index", "Home")</li>
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Services <span class="caret"></span></a>
                    <ul class="dropdown-menu">
                        <li>@Html.ActionLink("Preventive Maintenance", "Service", "Home", new {Id = 0}, null)</li>
                        <li>@Html.ActionLink("Certified Load Test", "Service", "Home", new {Id = 1}, null)</li>
                        <li>@Html.ActionLink("Inspections and Repairs", "Service", "Home", new {Id = 2}, null)</li>
                        <li>@Html.ActionLink("Relocation", "Service", "Home", new {Id = 3}, null)</li>
                        <li>@Html.ActionLink("Modifications and Upgrades", "Service", "Home", new {Id = 4}, null)</li>
                    </ul>
                </li>
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Popular Products <span class="caret"></span></a>
                    <ul class="dropdown-menu">
                        <li>@Html.ActionLink("Cranes", "Product", "Home", new {Id = 1}, null)</li>
                        <li>@Html.ActionLink("Pallet Racks", "Product", "Home", new {Id = 0}, null)</li>
                        <li>@Html.ActionLink("Loading Dock Equipment", "Product", "Home", new {Id = 2}, null)</li>
                        <li>@Html.ActionLink("Shelvings", "Product", "Home", new {Id = 3}, null)</li>
                        <li>@Html.ActionLink("Conveyors", "Product", "Home", new {Id = 4}, null)</li>
                        <li>@Html.ActionLink("Warehouse Mezzanines", "Product", "Home", new {Id = 5}, null)</li>
                    </ul>
                </li>
                <li>@Html.ActionLink("Complete Product Catalog", "Catalog", "Home", new {Id = 48}, null)</li>
                <li>@Html.ActionLink("Contact Us", "Contact", "Home")</li>
            </ul>

        </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
</nav>

<div class="container-fluid">
    @RenderBody();

</div>   
</body>
</html>
